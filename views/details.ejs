		<script>
			function myFunction() {
			  var x = document.getElementById("myInput");
			  if (x.type === "password") {
				x.type = "text";
			  } else {
				x.type = "password";
			  }
			}
			function regpass() {
				  var x = document.getElementById("reg");
				  if (x.type === "password") {
					x.type = "text";
				  } else {
					x.type = "password";
				  }
				}
			</script>
	</head>
	
<body class="green-skin bg-light">
<!-- Start Navigation navbar-->
