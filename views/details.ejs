									<li>
									<a href="#" data-toggle="modal" data-target="#login">
										<i class="ti-user mr-1"></i><span class="dn-lg">Login/Register</span>
									</a>
									</li>
									<% } %>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>

        <!-- end navbar  -->
        <!-- messages  -->
            <%- messages('messages', locals) -%>
        <div class="clearfix"></div>
			<!-- ============================================================== -->
			<!-- Top header  -->
			<!-- ============================================================== -->
			
			<!-- ======================= Start Banner ===================== -->
			<section class="page-title-banner"  style="background-image:url(/img/aa.jpg);">
				<div class="container">
					<div class="row ">
						<div class="tr-list-wrap">
							<div class="tr-list-detail">
								<div class="tr-list-info">
									<h2 style="color: white;"><%= user[0].name %></h2>
									<%= user[0].bio %>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>
			<!-- ======================= End Banner ===================== -->
		
			<!-- ============== Job Detail ====================== -->	
			<section class="tr-single-detail gray-bg">
				<div class="container">
					<div class="row">
					
						<div class="col-md-8 col-sm-12">
							
							
														
							<!-- Extra Skill& Knowledge -->
							<div class="tr-single-box">
								<div class="tr-single-header">
									<h4><i class="ti-book"></i>Extra Skill & Knowledge</h4>
								</div>
								<div class="tr-single-body">
									
									
									<!-- Skills -->
									<div class="row">

										<% user[0].skills.forEach(function(skill){ %>
											<div class="col-lg-6 col-md-6 col-sm-12">
												<label><%= skill.name %></label>
												<div class="progress">
												  <div class="progress-bar progress-bar-striped bg-warning" role="progressbar" style="width: <%= skill.level*20 %>%" aria-valuenow="98" aria-valuemin="0" aria-valuemax="100"></div>
												</div>
											</div>
											<% });%>
									</div>
									
								</div>
							</div>
							
							
							<!-- Education & Qualification -->
							<div class="tr-single-box">
								<div class="tr-single-header">
									<h4><i class="ti-cup"></i>Education & Qualification</h4>
								</div>
								<div class="tr-single-body">
									<ul class="qa-skill-list">
										<% user[0].education.forEach(function(a){ %>
										

											<li>
												<div class="qa-skill-box">
													<h4 class="qa-skill-title"><%= a.title %><span class="qa-time"><%= a.from %> - <%= a.to %></span></h4>
													<div class="qa-content">
														<p><b>University - </b><%= a.university %> <br>
														<b>Result - </b> <%= a.result%></p>
													</div>
												</div>
											</li>

											<% });%>
									
									</ul>
								</div>
							</div>
							
							<!-- Experience -->
							<div class="tr-single-box">
								<div class="tr-single-header">
									<h4><i class="ti-medall-alt"></i>Award & Experience</h4>
								</div>
								<div class="tr-single-body">
									<ul class="qa-skill-list">
										<% user[0].work.forEach(function(a){ %>
											<li>
												<div class="qa-skill-box">
													<h4 class="qa-skill-title"><%= a.orgname %><span class="qa-time"><%= a.start_date%> - <%= a.end_date %></span></h4>
													<h5 class="qa-subtitle"><%= a.title %></h5>
													<div class="qa-content">
														<p> <b>Project - <%= a.project_name %></b> <br>
														<b>Description - <%= a.description %> </b> <br>
														<b>Contribution - </b> <%= a.contribution %> <br>
														<b>Other Comments - </b> <%= a.comment %>
													</p>
													</div>
												</div>
											</li>

											
											<% });%>
									
										
									</ul>
								</div>
							</div>
						</div>
					
						<!-- Sidebar Start -->
						<div class="col-md-4 col-sm-12">
							
						
												
							<!-- Candidate Detail -->
							<div class="tr-single-box">
								<div class="tr-single-header">
									<h4><i class="ti-direction"></i> Candidate Details</h4>
								</div>
								
								<div class="tr-single-body">
									<ul class="extra-service">
										<li>
											<div class="icon-box-icon-block">
												<div class="icon-box-round">
													<i class="ti-stats-down"></i>
												</div>
												<div class="icon-box-text">
													<strong class="d-block">CGPA</strong>
													 <%= user[0].cgpa %> 
												</div>
											</div>
										</li>

										<li>
											<div class="icon-box-icon-block">
												<div class="icon-box-round">
													<i class="ti-user"></i>
												</div>
												<div class="icon-box-text">
													<strong class="d-block">Gender</strong>
													<%= user[0].gender %>
												</div>
											</div>
										</li>

										<li>
											<div class="icon-box-icon-block">
												<div class="icon-box-round">
													<i class="ti-pencil"></i>
												</div>
												<div class="icon-box-text">
													<strong class="d-block">Year</strong>
													<%= user[0].year %>
												</div>
											</div>
										</li>
										
										<li>
											<div class="icon-box-icon-block">
												<div class="icon-box-round">
													<i class="ti-world"></i>
												</div>
												<div class="icon-box-text">
													<strong class="d-block">Experience</strong>
													<%= user[0].experience %> years
												</div>
											</div>
										</li>
										
									</ul>
								</div>
								
							</div>
							
							<!-- Candidate Address -->
							<div class="tr-single-box">
								<div class="tr-single-header">
									<h4><i class="ti-direction"></i> Candidate Address</h4>
								</div>
								
								<div class="tr-single-body">
									<ul class="extra-service">
										<li>
											<div class="icon-box-icon-block">
												<a href="#">
													<div class="icon-box-round">
														<i class="ti-location-pin"></i>
													</div>
													<div class="icon-box-text">
														<%= user[0].location %>
													</div>
												</a>
											</div>
										</li>
										
										<li>
											<div class="icon-box-icon-block">
												<a href="#">
													<div class="icon-box-round">
														<i class="ti-bell"></i>
													</div>
													<div class="icon-box-text">
														<%= user[0].contact %>
													</div>
												</a>
											</div>
										</li>
										
										<li>
											<div class="icon-box-icon-block">
												<a href="#">
													<div class="icon-box-round">
														<i class="ti-google"></i>
													</div>
													<div class="icon-box-text">
														<%= user[0].email %>
													</div>
												</a>
											</div>
										</li>
										
										<!-- <li>
											<div class="icon-box-icon-block">
												<a href="#">
													<div class="icon-box-round">
														<i class="lni-world"></i>
													</div>
													<div class="icon-box-text">
														www.myfinding.com
													</div>
												</a>
											</div>
										</li> -->
										
									</ul>
								</div>
								
							</div>
							
							<!-- Social Links -->
							<div class="tr-single-box">
								<div class="tr-single-header">
									<h4><i class="ti-share"></i> Social Profiles</h4>
								</div>
								
								<div class="tr-single-body">
									<ul class="extra-service half">
										<li>
											<div class="icon-box-icon-block">
												<a href="#">
													<div class="icon-box-round">
														<i class="ti-linkedin"></i>
													</div>
													<div class="icon-box-text">
														
														<a href="<%= user[0].linkedin %>" target="_blank">Linkedin</a>
													</div>
												</a>
											</div>
										</li>
										
										<li>
											<div class="icon-box-icon-block">
												<a href="#">
													<div class="icon-box-round">
														<i class="ti-github"></i>
													</div>
													<div class="icon-box-text">
														
														<a href="<%= user[0].github %>" target="_blank">Github</a>
												</a>
											</div>
										</li>
										
										<li>
											<div class="icon-box-icon-block">
												<a href="#">
													<div class="icon-box-round">
														<i class="ti-twitter-alt"></i>
													</div>
													<div class="icon-box-text">
														<a href="" target="_blank">Twitter</a>
													</div>
												</a>
											</div>
										</li>
										
										
									</ul>
								</div>
								
							</div>
							
						</div>
						<!-- /col-md-4 -->
					</div>
				</div>
			</section>
			<!-- ============== Job Detail ====================== -->
			
					
			
			<!-- ============================ Footer Start ================================== -->
			
			<footer class="dark-footer skin-dark-footer">
				<div class="footer-bottom">
					<div class="container">
						<div class="row align-items-center">
							<div class="col-lg-6 col-md-6">
								<p class="mb-0">Â© 2020 Placement Portal. All Rights Reserved</p>
							</div>
							<div class="col-lg-6 col-md-6 text-right">
								<ul class="footer-bottom-social">
									<li><a href="#"><i class="ti-facebook"></i></a></li>
									<li><a href="#"><i class="ti-twitter"></i></a></li>
									<li><a href="#"><i class="ti-instagram"></i></a></li>
									<li><a href="#"><i class="ti-linkedin"></i></a></li>
								</ul>
							</div>
							
						</div>
					</div>
				</div>
			</footer>
			<!-- ============================ Footer End ================================== -->
	
			<!-- Log In Modal -->
			<div class="modal fade" id="login" tabindex="-1" role="dialog" aria-labelledby="registermodal" aria-hidden="true">
				<div class="modal-dialog modal-dialog-centered login-pop-form" role="document">
					<div class="modal-content" id="registermodal">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								<span aria-hidden="true"><i class="ti-close"></i></span>
							</button>
						</div>
						<div class="modal-body">
							<h4 class="modal-header-title">SignIn</h4>
							
							<div class="login-form">
								<form method="post" action="/users/login">
								
									<div class="form-group">
										<label>User Name</label>
										<div class="input-with-gray">
											<input type="text" class="form-control" placeholder="Username" name="username">
											<i class="ti-user theme-cl"></i>
										</div>
									</div>
									
									<div class="form-group">
										<label>Password</label>
										<input class="ml-40" type="checkbox" onclick="myFunction()">
										<div class="input-with-gray">
											<input type="password" name="password" id="myInput" class="form-control" placeholder="*******" required>
												<i class="ti-unlock theme-cl"></i>
										</div>
									</div>
									
									<div class="form-group">
										<button type="submit" class="btn btn-primary btn-md full-width pop-login">Login</button>
									</div>
								
								</form>
							</div>
						</div>
						<div class="modal-footer">
							<div class="mf-link"><i class="ti-user"></i>Don't have An Account?<a href="javascript:void(0)" data-toggle="modal" data-target="#signup" data-dismiss="modal"> Sign Up</a></div>
							<div class="mf-forget"><a href="#"><i class="ti-help"></i>Forget Password</a></div>
						</div>
					</div>
				</div>
			</div>
			<!-- End Modal -->
					
			<!-- Sign Up Modal -->
			<div class="modal fade" id="signup" tabindex="-1" role="dialog" aria-labelledby="sign-up" aria-hidden="true">
				<div class="modal-dialog modal-dialog-centered login-pop-form" role="document">
					<div class="modal-content" id="sign-up">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								<span aria-hidden="true"><i class="ti-close"></i></span>
							</button>
						</div>
						<div class="modal-body">
							<h4 class="modal-header-title">Sign Up</h4>
							
							
							<div class="login-form">
								<form method="post" action="/users/register">
									<div class="row">
										<div class="col-lg-6 col-md-6 col-sm-6">
											<div class="form-group">
												<label>Full Name</label>
												<div class="input-with-gray">
													<input type="text" class="form-control" placeholder="Your Name" name="name" required>
													<i class="ti-user theme-cl"></i>
												</div>
											</div>
										</div>
										
										<div class="col-lg-6 col-md-6 col-sm-6">
											<div class="form-group">
												<label>Email</label>
												<div class="input-with-gray">
													<input type="text" name="email" pattern="^[a-zA-Z0-9_]+@thapar\.edu$" class="form-control" placeholder="your_mail@thapar.edu" required>
													<i class="ti-email theme-cl"></i>
												</div>
											</div>
										</div>
									</div>
			
									<div class="row">
										<div class="col-lg-6 col-md-6 col-sm-6">
											<div class="form-group">
												<label>Year</label>
												<div class="input-with-gray">
													<input type="number" min="1" max="4" name="year" class="form-control" placeholder="Current year" required>
													<i class="ti-info-alt theme-cl"></i>
												</div>
											</div>
										</div>
										
										
									
										<div class="col-lg-6 col-md-6 col-sm-6">
											<div class="form-group">
												<label>Gender</label>
												<div class="input-with-gray">
													<select name="gender" id="" class="form-control" required>
														<option value="" disabled selected>Gender</option>
														<option value="Male">Male</option>
														<option value="Female">Female</option>
													</select>
													<i class="ti-basketball theme-cl"></i>
												</div>
											</div>
										</div>
										
								
										
									</div>
									<div class="row">
										<div class="col-lg-12 col-md-12 col-sm-12">
											<div class="form-group">
												<label>Roll no. </label>
												<div class="input-with-gray">
													<input type="text" name="username" pattern="^[0-9]*$" minlength="9" maxlength="9" class="form-control" placeholder="Roll no." required>
													<i class="ti-user theme-cl"></i>
												</div>
											</div>
										</div>
										<div class="col-lg-12 col-md-12 col-sm-12">
											<div class="form-group">
												<label>Password</label>
												<input class="ml-40" type="checkbox" onclick="regpass()">
												<div class="input-with-gray">
													<input type="password" name="password" id="reg" class="form-control" placeholder="*******" required>
												<i class="ti-unlock theme-cl"></i>
												</div>
											</div>
										</div>
									</div>
									
									<div class="row">
										<div class="col-lg-12 col-md-12 col-sm-12">
											<div class="form-group">
												<button type="submit" class="btn btn-primary btn-md full-width pop-login">Sign Up</button>
											</div>
										</div>
									</div>
			
								</form>
							</div>
						</div>
						
					</div>
				</div>
			</div>
			<!-- End Modal -->				
		

		</div>
		<!-- ============================================================== -->
		<!-- End Wrapper -->
		<!-- ============================================================== -->

