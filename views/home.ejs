        <!-- ============================================================== -->
        <!-- Top header  -->
		<!-- ============================================================== -->
		
        <!-- Start Navigation -->
        <div class="header header-light">
            <div class="container-fluid">
                <nav id="navigation" class="navigation navigation-landscape">
                    <div class="nav-header">
                        <a class="nav-brand" href="#">
                            <img src="/img/Logo.jpg" class="logo" width="170" height="40" alt="logo">
                        </a>
                        <div class="nav-toggle"></div>
                    </div>
                    <div class="nav-menus-wrapper" style="transition-property: none;">
                        <ul class="nav-menu">
                        
                            <li class="active"><a href="/">Home</a></li>
								<%if (user && user.admin){%>
									<li><a href="#">For Employers<span class="submenu-indicator"></span></a>
										<ul class="nav-dropdown nav-submenu" style="right: auto; display: none;">
											<li><a href="/admin/all/users">Browse Candidates Grid</a></li>                                    
	
											</li>
										</ul>
									</li>
									<%}%>
									<% if(user && !user.admin){ %>
										<li ><a href="#">For Candidates<span class="submenu-indicator"></span></a>
											<ul class="nav-dropdown nav-submenu" style="right: auto; display: none;">
											
												
													
												</li>
												
												<li><a href="/users/profile">Candidate Dashboard</a></li>	
												
											</ul>
										</li>
										<%}%>
							
                         
                            
                            <li><a href="#">Pages<span class="submenu-indicator"></span></a>
								<ul class="nav-dropdown nav-submenu" style="right: auto; display: none;">
									<% if(!user){%>
										<li><a href="/users/login">LogIn</a></li> 
										<%}%>
                                    
                                    <li><a href="/tip">Tips and Tricks</a></li>                                 
                                    
                                </ul>
                            </li>
                            
                            <li><a href="/contact">Contact Us</a></li>
                            
                        </ul>
                        
                        <ul class="nav-menu nav-menu-social align-to-right">
                            
                            
								<% if(user) {%>
									<li>
                                <a href="/users/logout" >
                                    <i class="ti-user mr-1"></i><span class="dn-lg">Logout</span>
								</a>
								</li>
								<% if(user.admin){ %>
									
									<li>
										<a href="/admin/messages" >
											<i class="ti-signal mr-1"></i><span class="dn-lg">Messages</span>
										</a>
									</li>
									<li class="add-listing theme-bg">
										<a href="/admin/all/users">
											 <i class="ti-plus"></i> Get Candidates
										</a>
									</li>
									<% }else{ %>
										<li class="add-listing theme-bg">
											<a href="/users/profile">
												 <i ></i> Update Profile
											</a>
										</li>
										<% } %>
								<% }else{ %>
									<li>
									<a href="#" data-toggle="modal" data-target="#login">
										<i class="ti-user mr-1"></i><span class="dn-lg">Login/Register</span>
									</a>
									</li>
									<% } %>
                           
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <!-- End Navigation -->
        <div class="clearfix"></div>
			<!-- ============================================================== -->
			<!-- Top header  -->
			<!-- ============================================================== -->
			
			<%- messages('messages', locals) -%>
			<!-- ============================ Hero Banner  Start================================== -->
